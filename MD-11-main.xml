							<command>nasal</command>
							<script>
							acconfig.about_dlg.open();
							</script>
						</binding>
					</item>
				</menu>
				<menu n="102">
					<label>AFS/FMS</label>
					<item>
						<label>AFS Panel</label>
						<binding>
							<command>dialog-show</command>
