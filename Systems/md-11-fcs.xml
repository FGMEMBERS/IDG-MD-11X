			<function>
				<sum>
					<ifthen>
						<or>
							<eq>
								<property>fcc/lsas/autotrim-switch</property>
								<value>1</value>
							</eq>
							<eq>
								<property>fcc/lsas/autotrim-switch</property>
								<value>3</value>
							</eq>
						</or>
						<value>0.00002</value>
						<value>0.0</value>
					</ifthen>
					<ifthen>
						<or>
							<eq>
								<property>fcc/lsas/autotrim-switch</property>
								<value>2</value>
							</eq>
							<eq>
								<property>fcc/lsas/autotrim-switch</property>
								<value>4</value>
							</eq>
						</or>
						<value>-0.00002</value>
						<value>0.0</value>
					</ifthen>
				</sum>
			</function>
		</fcs_function>
		
